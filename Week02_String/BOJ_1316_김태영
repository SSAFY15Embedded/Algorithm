#include <iostream>
#include <string>

using namespace std;

int main(){

    int DAT[100][100] = {0};
int n;
    int flag[100] = {0};
    cin >> n;
    string str[100];
    for (int i = 0; i < n; i++)
    cin >> str[i];
    
    for (int i = 0; i < n; i++){
        for (int j = 0; j < str[i].length(); j++){
            if (DAT[i][int(str[i][j])])
                flag[i] =1;
            if (str[i][j] != str[i][j+1]){
                DAT[i][int(str[i][j])] = 1;
}
}
}
int sum = 0;
    for (int i = 0; i < n; i++)
sum += flag[i];
    
    cout << n - sum;
    
    
}
